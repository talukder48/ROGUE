<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>RogueWeb</display-name>
  <welcome-file-list>
    
    <welcome-file>/WebPages/login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <description></description>
    <display-name>RogueServlet</display-name>
    <servlet-name>RogueServlet</servlet-name>
    <servlet-class>Rogue.RogueServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RogueServlet</servlet-name>
    <url-pattern>/WebPages/RogueServlet</url-pattern>
  </servlet-mapping>
</web-app>